<div class="container" id="container-box">
  <%= render "error_text" %>
  <div class="new-log-box">
    <%= form_for @log do |f| %>
      <div class="form-group row">
        <%= f.label :name_input, "名前", class: "col-2 col-form-label font-weight-bold" %>
        <div class="col-10">
          <%= f.text_field :name, class: "form-control", id: "log_name_input" %>
        </div>
      </div>

      <div class="form-group row">
        <%= f.label :size_input, "大きさ", class: "col-2 col-form-label font-weight-bold" %>
        <div class="col-10">
          <%= f.text_field :size, class: "form-control", id: "log_size_input" %>
        </div>
      </div>

      <div class="form-group row">
        <%= f.label :place_input, "場所", class: "col-2 col-form-label font-weight-bold" %>
        <div class="col-10">
          <%= f.text_field :place, class: "form-control", id: "log_place_input" %>
        </div>
      </div>

      <div class="form-group row">
        <%= f.label :image_input, "写真", class: "col-2 font-weight-bold" %>
        <div class="col-10">
          <%= f.file_field :image, id: "log_image_input" %>
        </div>
      </div>

      <div class="form-group row actions ">
        <%= f.submit :送信, class: "btn btn-info" %>
      </div>
    <% end %>
  </div>
</div>
<script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['GEOLOCATION_API_KEY'] %>" async defer>
</script>
