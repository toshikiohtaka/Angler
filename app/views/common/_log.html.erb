<div class="col-md-4">
  <img class="card-img-top" src="<%= log.image.url %>" alt="Card image cap">
  <div class="card-block">
    <h4 class="card-title">場所: <%= log.place %></h4>
    <p class="card-text">
      ・名前 <%= log.name %><br>
      ・大きさ <%= log.size %>
    </p>
    <p class="card-text"><small class="text-muted"><%= log.updated_at.to_s(:published_on) %></small></p>
  </div>
</div>
