<% @logs.each do |log| %>
  <li class="container" id="side-log-container">
    <div class="row">
      <div class="log-image col-md-5">
        <img src="<%= log.image.url %>">
      </div>
      <div class="col-md-7">
        <%= log.place %><br><hr class="line">
        サイズ : <%= log.size %>cm<br>
        名前 : <%= log.name %>
      </div>
    </div>
  </li>
<% end %>
